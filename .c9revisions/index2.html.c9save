{"ts":1351466953825,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"<!DOCTYPE html>\r\n<html>\r\n    <head>\r\n        <title> </title>\r\n        <script src=\"//ajax.googleapis.com/ajax/libs/jquery/1.8.1/jquery.min.js\"></script>\r\n        <script src=\"//ajax.googleapis.com/ajax/libs/jqueryui/1.8.23/jquery-ui.min.js\"></script>\r\n        <link rel=\"stylesheet\" type=\"text/css\" href=\"pin-styles.css\">\r\n        <link href=\"//netdna.bootstrapcdn.com/twitter-bootstrap/2.1.1/css/bootstrap-combined.min.css\" rel=\"stylesheet\">\r\n\r\n     \r\n        <style src=\"http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/themes/redmond/jquery-ui.css\"></style>\r\n    \r\n    \r\n        <script>\r\n        function revertDraggable(theListofMyDOMelements) {\r\n    theListofMyDOMelements.each(function() {\r\n        var $this = $(this),\r\n            position = $this.data(\"originalPosition\");\r\n//console.log ($this);\r\n//console.dir ($this);\r\n        if (position) {\r\n            $this.animate({\r\n                left: position.left,\r\n                top: position.top\r\n            }, 500, function() {\r\n                $this.data(\"originalPosition\", null);\r\n            });\r\n        }\r\n    });\r\n}\r\n\r\n$(document).ready(function() {\r\n    $(\".draggable\").draggable({\r\n        revert: \"invalid\", \r\n        snap: \".ui-widget-header\",\r\n        snapMode: \"both\",\r\n        grid: [ 30, 30 ]      \r\n    });\r\n    \r\n    $(\"#drop\").droppable({\r\n        drop: function(event, ui) {\r\n            if (!ui.draggable.data(\"originalPosition\")) {\r\n                ui.draggable.data(\"originalPosition\",\r\n                    ui.draggable.data(\"draggable\").originalPosition);\r\n            }\r\n        }\r\n    });\r\n\r\n    $(\"#undo\").click(function() {\r\n        // the values of $.drag retrieves a list of the DOM elements with class drag below\r\n        revertDraggable($(\".draggable\"));\r\n    });\r\n});\r\n\r\n\r\n$(document).ready(function() {\r\n \r\n $('#show_position').click(function(){\r\n \r\n var offset = $(\"#drag\").offset();\r\n var offset2 = $(\"#drag2\").offset();\r\n var offset3 = $(\"#drag3\").offset();\r\n var offset4 = $(\"#drag4\").offset();\r\n var offset5 = $(\"#drag5\").offset();\r\n var offset6 = $(\"#drag6\").offset();\r\n \r\n\r\n alert( \"House is at Top: \" + offset.top + \", \" +  \"Left: \" + offset.left +\r\n \",\\n Molly Top is \" + offset2.top + \", \" + \" Molly Left: \" + offset2.left +\r\n \",\\n Ardith Top is \" + offset3.top + \", \" + \" Ardith Left: \" + offset3.left +\r\n  \",\\n KEEP CALM Top is \" + offset4.top + \", \" + \" KEEP CALM Left: \" + offset4.left +\r\n  \",\\n TXT Top is \" + offset5.top + \", \" + \" TXT Left: \" + offset5.left +\r\n  \",\\n Crab Top is \" + offset6.top + \", \" + \" Crab Left: \" + offset6.left\r\n  \r\n  );\r\n \r\n });\r\n \r\n$('#show_filename').click(function(){\r\n    var x=document.getElementById(\"drag\").innerHTML;\r\n    var x2=document.getElementById(\"drag2\").innerHTML;\r\n    var x3=document.getElementById(\"drag3\").innerHTML;\r\n    var x4=document.getElementById(\"drag4\").innerHTML;\r\n    var x5=document.getElementById(\"drag5\").innerHTML;\r\n    var x6=document.getElementById(\"drag6\").innerHTML;\r\n    \r\n    alert( \"This is X =  \" + x +\r\n    \",\\n This is X2 =  \" + x2 +\r\n    \",\\n This is X3 =  \" + x3 +\r\n    \",\\n This is X4 =  \" + x4 +\r\n    \",\\n This is X5 =  \" + x5 +\r\n    \",\\n This is X6 =  \" + x6     \r\n    );\r\n     \r\n    \r\n});\r\n\r\n});\r\n\r\n\r\n\r\n  \r\n    \r\n        </script>\r\n    </head>\r\n<body>\r\n    <div class=\"header\"><img src=\"images/PinPixUploader-1.jpg\">\r\n    \r\n    </div>        \r\n    <div class=\"demo-description\">\r\n        <p>Drag and or all of the photos into the target rectangle. One box will allow you to type text.</p>\r\n    </div>\r\n    \r\n    <div class=\"container-fluid demo\">\r\n    <div class=\"row-fluid\">\r\n    <div class=\"span2\">\r\n    \r\n        <div id=\"drop\">The Drop Box</div>\r\n        <button id=\"undo\" type=\"button\">Undo</button>\r\n        <input type=\"submit\" name=\"show_position\" id=\"show_position\" value=\"Show DIV positions\" />\r\n     <input type=\"submit\" name=\"show_filename\" id=\"show_filename\" value=\"Show FILE NAMES\" />\r\n\r\n\r\n    <!--Sidebar content-->\r\n    </div>\r\n    <div class=\"span10\">\r\n\r\n    \r\n        <div id=\"drag\" class=\"draggable ui-widget-content\">\r\n            <img src=\"https://raw.github.com/metrony/pinxp/master/images/Beach-House-250px.jpg\"/>\r\n        </div>\r\n    \r\n        <div id=\"drag2\" class=\"draggable ui-widget-content\">\r\n            <img src=\"https://raw.github.com/metrony/pinxp/master/images/Molly-125px.jpg\"/>\r\n        </div>\r\n        \r\n         <div id=\"drag3\" class=\"draggable ui-widget-content\">\r\n        <img src=\"https://raw.github.com/metrony/pinxp/master/images/ardith-250px-by-350px.jpg\"/>\r\n        <!-- <p>Default (snap: true), snaps to all other draggable elements</p> -->\r\n    </div>\r\n\r\n    <div id=\"drag4\" class=\"draggable ui-widget-content\">\r\n        <img src=\"http://pintalk.net/wp-content/uploads/2012/09/Pinteresting.gif\"/>\r\n    </div>\r\n    \r\n     <div id=\"drag5\" class=\"draggable ui-widget-content\">\r\n        <textarea>Option to Add Text I snap to a 80 x 80 grid</textarea>\r\n    </div>\r\n\r\n    <div id=\"drag6\" class=\"draggable ui-widget-content\">\r\n    <img src=\"https://raw.github.com/metrony/pinxp/master/images/crab-250px.jpg\"/>\r\n        <!-- <p>I snap to a 20 x 20 grid</p> -->\r\n    </div>\r\n\r\n    \r\n   <!--Body content-->\r\n    </div>\r\n    </div>\r\n    </div>\r\n    \r\n    \r\n</body>\r\n</html>"]],"start1":0,"start2":0,"length1":0,"length2":5201}]],"length":5201}
